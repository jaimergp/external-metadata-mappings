  "package_managers": [
    {
      "name": "pkgcore",
      "commands": {
        "install": {
          "command": [
            "pmerge",
            "{}"
          ],
          "multiple_specifiers": "always",
          "requires_elevation": true
        },
        "query": {
          "command": [
            "pkgcore",
            "search",
            "-I",
            "{}"
          ]
        }
      },
      "specifier_syntax": {
        "exact_version": [
          "={name}-{version}"
        ],
        "name_only": [
          "{name}"
        ],
        "version_ranges": {
          "and": null,
          "equal": "={name}-{version}*",
          "greater_than": ">{name}-{version}",
          "greater_than_equal": ">={name}-{version}",
          "less_than": "<{name}-{version}",
          "less_than_equal": "<={name}-{version}",
          "not_equal": null,
          "syntax": [
            "{ranges}"
          ]
        }
      }
    },
